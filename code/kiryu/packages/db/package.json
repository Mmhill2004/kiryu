{
  "name": "@security-dashboard/db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "migrate": "wrangler d1 migrations apply security-dashboard-db --remote",
    "migrate:local": "wrangler d1 migrations apply security-dashboard-db --local",
    "seed": "wrangler d1 execute security-dashboard-db --file=./seed.sql --remote",
    "seed:local": "wrangler d1 execute security-dashboard-db --file=./seed.sql --local"
  },
  "devDependencies": {
    "wrangler": "^3.22.0"
  }
}
